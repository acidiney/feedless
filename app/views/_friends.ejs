<h2 style="margin-top: 30px">Friends</h2>

<% friends.friends.map(friend => { %>
  <a class="friend-item" href="<%= profileUrl(friend.id) %>">
    <img style="width: 32px; max-height: 100px; margin-right: 5px;" src="<%= profileImageUrl(friend) %>" />
    <div><%= friend.name %></div>
  </a>
<% }) %>